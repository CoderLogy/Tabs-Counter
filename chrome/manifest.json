{
    "name": "Tab Counter",
    "version": "1.1.0",
    "description": "Counts the Tabs",
    "manifest_version": 3,
    "author": "Shourya Wadhwa",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Tab Counter",
        "default_icon": {
            "512":"icons/tab.png"
    }},
    "permissions": [
            "tabs",
            "storage",
            "system.memory",
            "processes"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}